{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-airbnb"],
  "rules": {
    "variable-name": false,
    "member-access": false,
    "jsx-no-lambda": false,
    "import-name": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "align": false,
    "no-shadowed-variable": false
  }
}
